{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Traceback",
  "version": "0.1.0",
  "identifier": "net.hillsdon.matt.traceback",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Traceback",
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "width": 1400,
        "height": 900,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "infoPlist": "Info.plist",
      "entitlements": "Entitlements.plist"
    }
  }
}
